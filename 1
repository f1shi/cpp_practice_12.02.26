#include <iostream>
#include <string>

using namespace std;

int main() {
    string str = "0000";
    cin >> str;
    int ans = int(str[0]) - int(str[3]) + int(str[1]) - int(str[2]) + 1; 
//если 1 и 4 цифра равны, то а - б = 0; если 2 и 3 цифры равны, а - б = 0; +1 для проверки что a1-a4+a2-a3 = 0
    cout << ans;
}
